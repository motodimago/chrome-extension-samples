{
  "name": "04-menu_popup",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Sample context menu and pop up",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": ["contextMenus"]
}
